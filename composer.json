{
    "type": "library",
    "name": "sweetchuck/composer-suite-handler",
    "description": "Composer suite handler",
    "keywords": [],
    "authors": [
        {
            "name": "Andor",
            "role": "Maintainer",
            "homepage": "https://github.com/Sweetchuck"
        }
    ],
    "homepage": "https://github.com/Sweetchuck/composer-suite-handler",
    "support": {
        "source": "https://github.com/Sweetchuck/composer-suite-handler",
        "issues": "https://github.com/Sweetchuck/composer-suite-handler/issues"
    },
    "license": "LGPL-2.0-or-later",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "sweetchuck/git-hooks": true
        },
        "preferred-install": {
          "*": "dist"
        },
        "optimize-autoloader": true,
        "sort-packages": true
    },
    "repositories": {},
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "symfony/filesystem": "^4.4 || ^5.4 || ^6.0",
        "symfony/finder": "^4.4 || ^5.4 || ^6.0"
    },
    "require-dev": {
        "codeception/codeception": "^4.1",
        "codeception/module-asserts": "^1.3",
        "consolidation/robo": "^3.0",
        "mikey179/vfsstream": "^1.6",
        "nuvoleweb/robo-config": "2.x-dev",
        "phpstan/phpstan": "^1.5",
        "squizlabs/php_codesniffer": "^3.6",
        "sweetchuck/git-hooks": "2.x-dev",
        "sweetchuck/robo-git": "2.x-dev",
        "sweetchuck/robo-phpcs": "2.x-dev",
        "sweetchuck/robo-phpmd": "2.x-dev",
        "sweetchuck/robo-phpstan": "1.x-dev",
        "sweetchuck/utils": "1.x-dev",
        "symfony/error-handler": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "Sweetchuck\\ComposerSuiteHandler\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sweetchuck\\ComposerSuiteHandler\\Tests\\Acceptance\\": "tests/acceptance/",
            "Sweetchuck\\ComposerSuiteHandler\\Tests\\Unit\\": "tests/unit/",
            "Sweetchuck\\ComposerSuiteHandler\\Tests\\": "tests/_support/"
        }
    },
    "scripts": {},
    "scripts-descriptions": {},
    "extra": {}
}
